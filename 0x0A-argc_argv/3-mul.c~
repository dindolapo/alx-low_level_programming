#include <stdlib.h>
#include <stdio.h>
#include <errno.h>
#include <string.h>

extern int errno;

/**
 * main - check the code
 * @argc: arg count
 * @argv: arg vector
 * Return: Always 0 for success and 1 for error
 */
int main(int argc, char *argv[])
{
	int num1;
	int num2;
	int errnum;

	(void)argc;

	num1 = atoi(argv[1]);
	num2 = atoi(argv[2]);

	printf("%d\n", num1 * num2);

	if (argv == NULL)
	{
		errnum = errno;
		fprintf(stderr, "%d\n", errno);
		perror("Error");
		fprintf(stderr, "%s\n", strerror( errnum ));

		return (1);
	}
	else
		return (0);
}
