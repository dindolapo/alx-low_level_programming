#include "main.h"
#include <stdio.h>

/**
 * print_array - prints a specified number integers contained in an array
 * @a: input pointer argument
 * @n: input argument for numbers of elements to print
 * Return: void
 */
void print_array(int *a, int n)
{
	int i;

	for (i = 0; i < n; i++)
	{
		if (i == n - 1)
			printf("%d", a[i]);
		else
			printf("%d, ", a[i]);
	}
}
